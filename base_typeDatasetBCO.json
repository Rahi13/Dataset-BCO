{
    "name": "base_typeDatasetBCO",
    "title": "Base type for all Dataset BioCompute Object types",
    "description": "All Dataset BioCompute object types must inherit from this type in order to be compliant with BioCompute framework",
    "_comment": [
        "Since JSON format do not allow comments '_comment' key is used for comments and should be ignored by parsers",
        "As any object of type 'type' it has its own fields: _type, _id, _inherits, name, title and description",
        "As base type this type inherits from none",
        "Identifier of the object since type itself is an object",
        "Type of this JSON object is 'type'",
        "For each field default value for '_multi_value', '_optional', and '_read_only' key is 'false' "
    ],
    "_fields": {
        "_comment": "List of fields of the BioCompute Object type",
        "provenance_domain": {
            "name": {
                "_type": "string",
                "_comment": "Searchable file name for the Dataset"
            },
            "title": {
                "_type": "string",
                "_comment": "Short title for the dataset"
            },
            "version": {
                "_type": "version",
                "_comment": "Version of the Dataset BioCompute Object "
            },
            "digital_signature": {
                "_type": "blob",
                "_comment": "Digital signature of BioCompute Object by Authority",
                "_read_only": true
            },
            "review": {
                "_multi_value": true,
                "status": {
                    "_type": "string",
                    "_constraint": {
                        "_choice": [
                            "unreviewed",
                            "in-review",
                            "approved",
                            "rejected",
                            "suspended"
                        ]
                    },
                    "_comment": "Current verification status of the BioCompute Object",
                    "_default_value": "unreviewed"
                },
                "reviewer": {
                    "_type": "contributor",
                    "_multi_value": true
                },
                "comment": {
                    "_type": "string",
                    "_optional": true,
                    "_multi_value": true
                },
                "_optional": true
            },
            "derived_from": {
                "parent_glygen_object": {
                    "_type": "objectid",
                    "_optional": true
                }
            },
            "obsolete": {
                "_type": "datetime",
                "_optional": true
            },
            "embargo": {
                "start_time": {
                    "_type": "datetime",
                    "_optional": true
                },
                "end_time": {
                    "_type": "datetime",
                    "_optional": true
                }
            },
            "created": {
                "_type": "datetime",
                "_comment": "Date and time of the Dataset BioCompute Object creation",
                "_read_only": true
            },
            "modified": {
                "_type": "datetime",
                "_comment": "Date and time of the Dataset BioCompute Object was last modified",
                "_read_only": true,
                "_multi_value": true
            },
            "contributors": {
                "_type": "contributor",
                "_multi_value": true
            },
            "license": {
                "_type": "uri",
                "_comment": "Creative Commons licence or other licence information space. Recommended licence can be Attribution 4.0"
            }
        },
        "usability_domain": {
            "_type": "string",
            "_comment": "Text from biospec",
            "_multi_value": true
        },
        "description domain": {
            "keywords": {
                "_type": "keymap",
                "_multi_value": true
            },
            "xref": {
                "_type": "xref",
                "_multi_value": true
            },
            "pipeline_steps": {
                "_layout": "array",
                "tool": {
                    "_multi_value": true,
                    "name": {
                        "_type": "string",
                        "_comment": "this is a recognized name of the software tool"
                    },
                    "description": {
                        "_type": "string"
                    },
                    "step_number": {
                        "_type": "integer"
                    },
                    "version": {
                        "_type": "version"
                    },
                    "prerequisite": {
                        "_comment": "reference or required prereqs",
                        "name": {
                            "_type": "string",
                            "_comment": "Public searchable name for reference or prereq"
                        },
                        "URI": {
                            "_type": "uri"
                        },
                        "_multi_value": true
                    },
                    "input_list": {
                        "_type": "uri",
                        "_multi_value": true
                    },
                    "output_list": {
                        "_type": "uri",
                        "_multi_value": true
                    }
                }
            }
        },
        "execution_domain": {
            "script_type": {
                "_type": "string",
                "_constraint": {
                    "_choice": [
                        "url",
                        "text"
                    ]
                }
            },
            "script": {
                "_type": "string"
            },
            "script_driver": {
                "_type": "string",
                "_optional": true
            },
            "script_location": {
                "_type": "uri"
            },
            "pipeline_version": {
                "_type": "version",
                "_optional": true
            },
            "platform": {
                "_type": "string"
            },
            "software_prerequisites": {
                "_multi_value": true,
                "name": {
                    "_type": "string"
                },
                "version": {
                    "_type": "version"
                },
                "uri": {
                    "_type": "uri",
                    "_optional": true
                }
            },
            "domain_prerequisites": {
                "_multi_value": true,
                "name": {
                    "_type": "string"
                },
                "url": {
                    "_type": "url",
                    "_multi_value": true
                }
            },
            "environmental_parameters": {
                "_type": "keyval",
                "_comment": "all fields in this here should be defined in inheriting BioCompute subtypes",
                "_multi_value": true,
                "_optional": true
            }
        },
        "io_domain": {
            "input_subdomain": {
                "_type": "object",
                "_multi_value": true,
                "_comment": "all fields in this domain are BioCompute specific and should be defined in inherited BioCompute type",
                "_fileds": {
                    "name": {
                        "_type": "string",
                        "_comment": ""
                    },
                    "source": {
                        "_type": "file"
                    }
                }
            },
            "output_subdomain": {
                "_type": "file",
                "_multi_value": true,
                "_comment": "output is a file object"
            },
            "column_headers": {
                "_type": "object",
                "_multi_value": true,
                "_comment": "Key-value pairs that describe the content of the output data file"
            },
            "error_domain": {
                "empirical_error": {
                    "stats": {
                        "_type": "object",
                        "_multi_value": true,
                        "_comment": "Key-value pairs that describe the content of the output data file"
                    }
                }
            }
        }
    }
}
